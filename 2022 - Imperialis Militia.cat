  <sharedRules>
    <rule id="46041792-1ccb-499d-b7bc-92bdab6804fd" name="Militia Cavalry Mount" publicationId="6b825422-7e3d-4d9c-8ad6-a24da15fc9e8" page="37" hidden="false"/>
